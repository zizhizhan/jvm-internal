<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
   
	<servlet>
		<servlet-name>hello</servlet-name>
		<servlet-class>com.zizhizhan.legacies.pattern.mvc.HelloServlet</servlet-class>
	</servlet>
  
	<servlet>
		<servlet-name>mvcmini</servlet-name>
		<servlet-class>com.zizhizhan.legacies.pattern.mvc.v0.MVCServlet</servlet-class>
	</servlet>
  
	<servlet>
		<servlet-name>mvc</servlet-name>
		<servlet-class>com.zizhizhan.legacies.pattern.mvc.v1.DispatcherServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>hello</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>mvcmini</servlet-name>
		<url-pattern>/mvcmini/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>mvc</servlet-name>
		<url-pattern>/mvc/*</url-pattern>
	</servlet-mapping>
  
</web-app>